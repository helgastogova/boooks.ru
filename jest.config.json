{
  "timers": "fake",
  "snapshotSerializers": ["enzyme-to-json/serializer"],
  "moduleFileExtensions": ["js", "ts", "tsx", "json"],
  "roots": ["<rootDir>/src"],
  "moduleDirectories": ["node_modules"],
  "transformIgnorePatterns": [
    "node_modules/@babel/cli",
    "node_modules/@babel/core",
    "node_modules/@babel/plugin-transform-typescript",
    "node_modules/@babel/preset-env",
    "node_modules/@babel/preset-react",
    "node_modules/@babel/preset-typescript",
    "node_modules/@storybook/addon-actions",
    "node_modules/@storybook/addon-links",
    "node_modules/@storybook/addons",
    "node_modules/@storybook/react",
    "node_modules/@types/node",
    "node_modules/@types/react-css-modules",
    "node_modules/@types/react-dom",
    "node_modules/@types/webpack",
    "node_modules/@types/webpack-dev-server",
    "node_modules/babel-eslint",
    "node_modules/babel-jest",
    "node_modules/babel-loader",
    "node_modules/babel-plugin-react-css-modules",
    "node_modules/babel-preset-react",
    "node_modules/classnames",
    "node_modules/cross-env",
    "node_modules/css-loader",
    "node_modules/enzyme",
    "node_modules/enzyme-to-json",
    "node_modules/eslint",
    "node_modules/eslint-plugin-prettier",
    "node_modules/html-webpack-plugin",
    "node_modules/html-webpack-root-plugin",
    "node_modules/jest",
    "node_modules/jest-cli",
    "node_modules/jest-junit",
    "node_modules/jest-transform-css",
    "node_modules/postcss-loader",
    "node_modules/postcss-scss",
    "node_modules/prettier",
    "node_modules/pretty-quick",
    "node_modules/react",
    "node_modules/react-dom",
    "node_modules/react-scripts",
    "node_modules/react-test-renderer",
    "node_modules/redux-devtools",
    "node_modules/storybook",
    "node_modules/style-loader",
    "node_modules/ts-loader",
    "node_modules/ts-node",
    "node_modules/tsconfig-paths",
    "node_modules/typescript",
    "node_modules/webpack",
    "node_modules/webpack-cli",
    "node_modules/webpack-dev-server"
  ],
  "unmockedModulePathPatterns": ["node_modules/*", "^.+\\.module\\.css$"],
  "transform": {
    "\\.tsx?$": "babel-jest",
    "\\.module.(css)$": "jest-transform-css"
  }
}
